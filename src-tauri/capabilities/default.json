{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "editor",
    "settings"
  ],
  "permissions": [
    "core:default",
    "core:window:allow-set-theme",
    "shell:allow-open",
    "store:allow-get",
    "store:allow-set",
    "store:allow-save",
    "store:allow-load",
    "dialog:default",
    "fs:default",
    "fs:allow-app-read",
    "fs:allow-app-write"
  ]
}